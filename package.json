{
    "name": "csharp-comment-finder",
    "displayName": "C# Comment Finder",
    "description": "Find comments in .cs files",
    "version": "0.0.1",
    "publisher": "tkaluzny",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.findComments"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.findComments",
            "title": "Find Comments in C# files"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}